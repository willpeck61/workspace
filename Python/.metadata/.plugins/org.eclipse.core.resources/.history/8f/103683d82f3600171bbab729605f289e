import math

def exponent(aval, bval, fval):
    aval *= 2
    bval /= 2
    bval = math.floor(bval)
    if (bval % 2) != 0:
        fval += aval
    print(aval, " ", bval)
    if (bval != 1):
        return exponent(aval, bval, fval)
    else:
        return fval
    
